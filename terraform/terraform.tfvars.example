  zendesk = {
    client_id     = "your-zendesk-client-id"
    client_secret = "your-zendesk-client-secret"
    enabled       = true
  }
  calendly = {
    client_id     = "your-calendly-client-id"
    client_secret = "your-calendly-client-secret"
    enabled       = true
  }
