# Production Environment Configuration for OpsAI Onboarding

# Core API Keys (REQUIRED)
OPENAI_API_KEY=sk-your-openai-api-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key

# Application URLs
NEXT_PUBLIC_APP_URL=https://app.opsai.com
NEXT_PUBLIC_API_URL=https://api.opsai.com
NEXT_PUBLIC_WEBHOOK_URL=https://webhooks.opsai.com

# Supabase Configuration (REQUIRED)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_MANAGEMENT_API_KEY=your-management-api-key
SUPABASE_MANAGEMENT_API_URL=https://api.supabase.com

# Database
DATABASE_URL=postgresql://user:password@host:5432/dbname

# Airbyte Configuration (REQUIRED)
AIRBYTE_API_URL=https://api.airbyte.com/v1
AIRBYTE_CLIENT_ID=your-client-id
AIRBYTE_CLIENT_SECRET=your-client-secret
AIRBYTE_WORKSPACE_ID=your-workspace-id

# Security (REQUIRED - Generate with: openssl rand -hex 32)
CREDENTIAL_ENCRYPTION_KEY=your-32-character-hex-key
JWT_SECRET=your-jwt-secret
SESSION_SECRET=your-session-secret

# OAuth Providers (REQUIRED for each enabled provider)
# Google
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=https://app.opsai.com/api/oauth/google/callback

# GitHub
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
GITHUB_APP_ID=your-github-app-id
GITHUB_PRIVATE_KEY=your-github-private-key
GITHUB_WEBHOOK_SECRET=your-github-webhook-secret

# Stripe
STRIPE_CLIENT_ID=your-stripe-client-id
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

# Shopify
SHOPIFY_CLIENT_ID=your-shopify-client-id
SHOPIFY_CLIENT_SECRET=your-shopify-client-secret
SHOPIFY_APP_URL=https://app.opsai.com

# Deployment Platforms
# Vercel
VERCEL_TOKEN=your-vercel-token
VERCEL_TEAM_ID=your-vercel-team-id

# Railway
RAILWAY_TOKEN=your-railway-token
RAILWAY_PROJECT_ID=your-railway-project-id

# Netlify
NETLIFY_TOKEN=your-netlify-token
NETLIFY_SITE_ID=your-netlify-site-id

# Monitoring & Analytics
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
DATADOG_API_KEY=your-datadog-api-key
MIXPANEL_TOKEN=your-mixpanel-token
POSTHOG_API_KEY=your-posthog-api-key
POSTHOG_HOST=https://app.posthog.com

# Email Service
SENDGRID_API_KEY=your-sendgrid-api-key
RESEND_API_KEY=your-resend-api-key
EMAIL_FROM=noreply@opsai.com

# Queue & Cache
REDIS_URL=redis://user:password@host:6379
BULLMQ_REDIS_URL=redis://user:password@host:6379

# File Storage
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
S3_BUCKET_NAME=opsai-production

# Rate Limiting
UPSTASH_REDIS_REST_URL=https://your-instance.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-upstash-token

# Feature Flags
LAUNCHDARKLY_SDK_KEY=your-launchdarkly-key
FLAGSMITH_ENVIRONMENT_KEY=your-flagsmith-key

# Agent Service
AGENT_SERVICE_URL=https://agents.opsai.com
NEXT_PUBLIC_AGENT_WS_URL=wss://agents.opsai.com/ws

# Web Scraping & Analysis
BROWSERLESS_API_KEY=your-browserless-key
SCRAPINGBEE_API_KEY=your-scrapingbee-key
APIFY_TOKEN=your-apify-token

# Additional OAuth Providers
QUICKBOOKS_CLIENT_ID=your-quickbooks-client-id
QUICKBOOKS_CLIENT_SECRET=your-quickbooks-client-secret

HUBSPOT_CLIENT_ID=your-hubspot-client-id
HUBSPOT_CLIENT_SECRET=your-hubspot-client-secret

SALESFORCE_CLIENT_ID=your-salesforce-client-id
SALESFORCE_CLIENT_SECRET=your-salesforce-client-secret

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
AUDIT_LOG_ENABLED=true

# Performance
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1