{
  "name": "demo-saas-app",
  "version": "1.0.0",
  "description": "A demonstration SaaS application with core features",
  "main": "dist/api/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/api/server.ts",
    "start": "node dist/api/server.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integrations": "tsx scripts/test-integrations.ts",
    "test:all": "npm run test && npm run test:integrations",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type:check": "tsc --noEmit",
    "workflow:start": "tsx src/workflows/service.ts",
    "temporal:start": "cd temporal && docker-compose up -d",
    "temporal:stop": "cd temporal && docker-compose down",
    "temporal:worker": "ts-node activities/worker.ts",
    "temporal:ui": "open http://localhost:8080",
    "alerts:start": "tsx src/alerts/service.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:seed": "supabase db seed",
    "supabase:migrate": "supabase db migrate up",
    "supabase:types": "supabase gen types typescript --local > types/supabase.ts",
    "temporal:logs": "cd temporal && docker-compose logs -f",
    "temporal:reset": "cd temporal && docker-compose down -v && docker-compose up -d"
  },
  "dependencies": {
    "@opsai/shared": "workspace:*",
    "@opsai/auth": "workspace:*",
    "@opsai/workflow": "workspace:*",
    "@opsai/integration": "workspace:*",
    "@opsai/alerts": "workspace:*",
    "@opsai/files": "workspace:*",
    "@opsai/database": "workspace:*",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "@prisma/client": "^5.7.0",
    "zod": "^3.22.0",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "@supabase/supabase-js": "^2.38.0",
    "jsonwebtoken": "^9.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "bullmq": "^5.0.0",
    "@temporalio/client": "^1.8.0",
    "@temporalio/worker": "^1.8.0",
    "@temporalio/workflow": "^1.8.0",
    "@temporalio/activity": "^1.8.0",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.7",
    "redis": "^4.6.0",
    "node-cron": "^3.0.3",
    "multer": "^1.4.5",
    "@types/multer": "^1.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/morgan": "^1.9.0",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0",
    "prisma": "^5.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@types/node-cron": "^3.0.0",
    "supabase": "^1.130.0"
  },
  "author": "OpsAI Platform",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "opsai",
    "supabase",
    "temporal",
    "airbyte",
    "analytics",
    "business-intelligence",
    "workflow-orchestration",
    "data-integration",
    "vertical-saas",
    "demo-saas-app"
  ]
}